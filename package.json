{
    "name": "aurelia-medium-editor",
    "version": "0.1.11",
    "description": "medium-editor plugin for Aurelia.",
    "keywords": [
        "aurelia",
        "plugin",
        "medium",
        "editor",
        "WYSIWYG"
    ],
    "bugs": {
        "url": "https://github.com/arabsight/aurelia-medium-editor/issues"
    },
    "license": "MIT",
    "author": "Rabah Ghodbane <r-ghodbane@live.com> (http://arabsight.github.io/)",
    "main": "dist/index.js",
    "repository": {
        "type": "git",
        "url": "http://github.com/arabsight/aurelia-medium-editor"
    },
    "scripts": {
        "prepush": "gulp prepare-release --bump=patch",
        "push": "git add --all && git commit -m \"$(date)\" && git push origin master && npm publish",
        "test": "gulp test"
    },
    "devDependencies": {
        "aurelia-tools": "^1.0.0",
        "babel-eslint": "^8.0.1",
        "babel-plugin-syntax-flow": "^6.18.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-es2015-modules-amd": "^6.24.1",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
        "babel-plugin-transform-es2015-modules-systemjs": "^6.24.1",
        "babel-plugin-transform-flow-strip-types": "^6.22.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-1": "^6.24.1",
        "conventional-changelog": "1.1.6",
        "del": "^3.0.0",
        "gulp": "^3.9.1",
        "gulp-babel": "^7.0.0",
        "gulp-bump": "^2.7.0",
        "gulp-eslint": "^4.0.0",
        "gulp-yuidoc": "^0.1.2",
        "isparta": "^4.0.0",
        "istanbul": "^1.0.0-alpha.2",
        "jasmine-core": "^2.8.0",
        "karma": "^1.7.1",
        "karma-babel-preprocessor": "^7.0.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage": "^1.1.1",
        "karma-jasmine": "^1.1.0",
        "karma-jspm": "2.2.3",
        "medium-editor": "^5.23.2",
        "object.assign": "^4.0.4",
        "require-dir": "^0.3.2",
        "run-sequence": "^2.2.0",
        "vinyl-paths": "^2.1.0",
        "yargs": "^9.0.1"
    },
    "jspm": {
        "registry": "npm",
        "main": "index",
        "format": "cjs",
        "directories": {
            "dist": "dist"
        },
        "devDependencies": {
            "aurelia-framework": "^1.0.0",
            "aurelia-polyfills": "^1.0.0"
        }
    },
    "aurelia": {
        "build": {
            "resources": [
                "index",
                "medium-editor-plugin"
            ]
        }
    }
}
